<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mx.common.dao.IFrameDao" >
    <resultMap id="BaseResultMap" type="com.mx.generator.pojo.SysUser" >
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="true_name" property="trueName" jdbcType="VARCHAR" />
        <result column="department_id" property="departmentId" jdbcType="INTEGER" />
        <result column="role_ids" property="roleIds" jdbcType="VARCHAR" />
        <result column="user_name" property="userName" jdbcType="VARCHAR" />
        <result column="avatar" property="avatar" jdbcType="VARCHAR" />
        <result column="memo" property="memo" jdbcType="VARCHAR" />
        <result column="enablef" property="enablef" jdbcType="TINYINT" />
    </resultMap>
  <select id="logining" resultMap="BaseResultMap" parameterType="map" >
    SELECT
        id,
        true_name,
        user_name,
        department_id,
        role_ids,
        avatar
    FROM
        sys_user
    WHERE
        user_name = #{userName}
        AND password = #{password}
        AND deletef = 0
        AND enablef = 0
  </select>
  
</mapper>